{
    "name": "ajhaupt7/image-upload-preview",
    "description": "A Laravel Nova field for dragging and dropping images to upload, along with a preview.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ajhaupt7\\ImageUploadPreview\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajhaupt7\\ImageUploadPreview\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
